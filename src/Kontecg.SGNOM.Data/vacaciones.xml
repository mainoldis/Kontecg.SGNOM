<FCs>
	<FC Nombre="C_Pvac">
		<CLASIFICADOR Nombre="1">
			<CASE Condicion="DOCTYPE = 3 |DOCTYPE = 20 | DOCTYPE = 25">
				<CASE Condicion="TRANS=1">
					<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					<WRITE NV1="GETCTA(DESRET)" NV2="GETSCTA(DESRET)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
				</CASE>
			</CASE>
			<CASE Condicion="DOCTYPE = 7">
				<IF Condicion="CAJ" />
				<ELSE>
					<CASE Condicion="TRANS=2">
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
						<WRITE NV1="GETCTA(EXTNOM)" NV2="GETSCTA(EXTNOM)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					</CASE>
					<CASE Condicion="TRANS=3">
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="0" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
						<WRITE NV1="GETCTA(SAL_NREC)" NV2="GETSCTA(SAL_NREC)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					</CASE>
					<CASE Condicion="TRANS=4">
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
						<WRITE NV1="GETCTA(DESRET)" NV2="GETSCTA(DESRET)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					</CASE>
				</ELSE>
			</CASE>
		</CLASIFICADOR>
		<CLASIFICADOR Nombre="2">
			<CASE Condicion="DOCTYPE = 3 |DOCTYPE = 20 | DOCTYPE = 25">
				<CASE Condicion="TRANS=1">
					<CASE Condicion="TRANS=1">
						<WRITE NV1="GETCTA(DESRET)" NV2="GETSCTA(DESRET)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
						<WRITE NV1="CNT" NV2="CC" NV3="GETEGSTO(DESRET)" NV4="CHAPA" EXP="imp" ACCION="DEBITO" />
					</CASE>
				</CASE>
			</CASE>
		</CLASIFICADOR>
	</FC>
</FCs>