<?xml version="1.0" encoding="utf-8" ?>
<FCs>
	<FC Nombre="C_PPen">
		<CLASIFICADOR Nombre="1">
			<CASE Condicion="DOCTYPE = 4">
				<WRITE NV1="GETCTA(R_PALIM)" NV2="GETSCTA(R_PALIM)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
				<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
			</CASE>
			<CASE Condicion="DOCTYPE = 8">
				<IF Condicion="CAJ" />
				<ELSE>
					<CASE Condicion="TRANS=2">
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
						<WRITE NV1="GETCTA(EXTNOM)" NV2="GETSCTA(EXTNOM)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					</CASE>
					<CASE Condicion="TRANS=3 | TRANS=4">
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="0" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
						<WRITE NV1="GETCTA(R_PALIM)" NV2="GETSCTA(R_PALIM)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
					</CASE>
				</ELSE>
			</CASE>
		</CLASIFICADOR>
		<CLASIFICADOR Nombre="2">
			<CASE Condicion="DOCTYPE = 4">
				<WRITE NV1="GETCTA(R_PALIM)" NV2="GETSCTA(R_PALIM)" NV3="0" NV4="CHAPA" EXP="imp" ACCION="DEBITO" />
