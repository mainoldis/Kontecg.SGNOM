<FCs>
	<FC Nombre="C_RECLAM">
		<CLASIFICADOR Nombre="1">
			<CASE Condicion="DOCTYPE = 5 | DOCTYPE =25  ">
				<CASE Condicion="TRANS=1">
					<WRITE NV1="GETCTA(SAL_NREC)" NV2="GETSCTA(SAL_NREC)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
					<WRITE NV1="GETCTA(NOMPAGAR)" NV2="GETSCTA(NOMPAGAR)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
				</CASE>
			</CASE>
			<CASE Condicion="DOCTYPE = 6">
				<IF Condicion="CAJ" />
				<ELSE>
					<CASE Condicion="TRANS=2">
						<WRITE NV1="GETCTA(EXTNOM)" NV2="GETSCTA(EXTNOM)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
						<WRITE NV1="GETCTA(NOMPAGAR)" NV2="0" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
					</CASE>
					<CASE Condicion="TRANS=3">
						<WRITE NV1="GETCTA(SAL_NREC)" NV2="GETSCTA(SAL_NREC)" NV3="0" NV4="0" EXP="imp" ACCION="CREDITO" />
						<WRITE NV1="GETCTA(EXTNOM)" NV2="GETSCTA(EXTNOM)" NV3="0" NV4="0" EXP="imp" ACCION="DEBITO" />
					</CASE>
				</ELSE>
			</CASE>
		</CLASIFICADOR>
	</FC>
